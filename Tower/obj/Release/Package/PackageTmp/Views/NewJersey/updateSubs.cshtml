
@{
    ViewBag.Title = "Subsequent Updates";
}

<h2>Subsequent Updates</h2>

<h1>@ViewBag.Message</h1>
<p>This is the layout for the upload. Save as Excel (.xls)</p>
<img src="~/Images/Layout.jpg" />

@using (Html.BeginForm( "updateSubs","NewJersey", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
     <div class="form-group">
        <div class="col-md-10">
            <input type="file" name="file" />
        </div>
    </div>
     <div class="form-group">
        <div class="col-md-10">
            <input type="submit" value="Upload File" class="btn btn-primary" />
        </div>
    </div>
    
}